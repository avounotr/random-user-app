{
  "extends": "airbnb",
  "rules": {
    "react/no-danger": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/anchor-has-content": 0,
    // Rules.
    // "comma-dangle": 0
    "no-console": [
      2,
      {
        "allow": ["info", "warn", "error"]
      }
    ]
  },
  "globals": {
    "document": true,
    "window": true,
    "history": true,
    "location": true,
    "screen": true,
    "describe": true,
    "beforeEach": true,
    "before": true,
    "it": true,
    "performance": true,
    "plyr": true
  }
}
